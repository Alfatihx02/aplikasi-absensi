<%- include('../partials/head') %>
<div class="main-panel">
    <div class="content-wrapper d-flex align-items-center auth px-0">
      <div class="row w-100 mx-0">
        <div class="col-md-6 grid-margin stretch-card">
          <div class="card">
            <div class="card-body">
              <h4 class="card-title">Form Kelas</h4>
              <form class="forms-sample" action="/kelas/store" method="POST">
                <div class="form-group">
                  <label for="exampleInputUsername1">Kelas</label>
                  <input type="text" class="form-control" id="exampleInputUsername1" placeholder="kelas Jurusan" name="nama_kelas">
                </div>
                <div class="form-group">
                    <label for="id_user">ID PRODI</label>
                    <select class="form-select" id="exampleSelectIdUser" name="id_prodi">
                      <% for( let i = 0; i < data.length; i++ ) { %>  
                        <option value="<%= data[i].id_prodi %>"><%= data[i].nama_prodi %></option>
                      <% } %>
                    </select>
                  </div>
                <button type="submit" class="btn btn-primary me-2">Submit</button>
                <a href="/admin/data_kelas" class="btn btn-secondary me-5">Kembali</a>
              </form>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

<%- include('../partials/foot') %>