<%- include('../partials/head') %>
<div class="main-panel">
    <div class="content-wrapper d-flex align-items-center auth px-0">
      <div class="row w-100 mx-0">
        <div class="col-md-6 grid-margin stretch-card">
          <div class="card">
            <div class="card-body">
              <h4 class="card-title">Form Ruangan</h4>
              <form class="forms-sample" action="/ruangan/update/<%= id_r %>" method="POST">
                <div class="form-group">
                  <label for="exampleInputUsername1">Ruangan</label>
                  <input type="text" class="form-control" id="exampleInputUsername1" placeholder="Nama Ruangan" name="nama_ruangan" value="<%= nama_ruangan %>">
                </div>
                <button type="submit" class="btn btn-primary me-2">Submit</button>
                <a href="/admin/data_ruangan" class="btn btn-secondary me-5">Kembali</a>
              </form>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

<%- include('../partials/foot') %>